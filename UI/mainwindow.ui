  <property name="styleSheet">
   <string notr="true">* {
	font-family: consolas;
	color: rgb(200, 200, 200);
	background-color: rgb(35, 35, 35);
}

/* QLabel */
QLabel.standard {
	font-size: 10pt;
	background-color: transparent;
}

/* Menubar */
QMenuBar {
	background: rgb(50, 50, 50);
}

QMenuBar::item  {
    background-color: transparent;
	color: rgb(200, 200, 200);
}

QMenuBar::item:selected {
    background-color: rgb(100, 100, 100);
}

QMenuBar::item:pressed {
    background-color: rgb(200, 200, 200);
	color: rgb(20, 20, 20);
}

QMenuBar::item:disabled {
    background-color:transparent;
	color: rgb(100, 100, 100);
}

/* QMenu */
QMenu {
	background: rgb(50, 50, 50);
}

QMenu::separator {
	height: 1px;
	background-color: rgb(100, 100, 100);
	margin: 5px, 5px, 5px, 5px;
}

QMenu:disabled {
	color: rgb(100, 100, 100);
}

/*QMenu::indicator:non-exclusive:unchecked {
	image: url(:/Resources/Images/checkbox_off.png);
}

QMenu::indicator:non-exclusive:unchecked:selected {
	image: url(:/Resources/Images/checkbox_off.png);
}

QMenu::indicator:non-exclusive:checked {
	image: url(:/Resources/Images/checkbox_on.png);
}

QMenu::indicator:non-exclusive:checked:selected {
	image: url(:/Resources/Images/checkbox_on.png);
}

QMenu::indicator:exclusive:unchecked {
	image: url(:/Resources/Images/radiobutton_off.png);
}

QMenu::indicator:exclusive:unchecked:selected {
	image: url(:/Resources/Images/radiobutton_off.png);
}

QMenu::indicator:exclusive:checked {
	image: url(:/Resources/Images/radiobutton_on.png);
}

QMenu::indicator:exclusive:checked:selected {
	image: url(:/Resources/Images/radiobutton_on.png);
}*/

QAction:disabled {
	color: rgb(100, 100, 100);
}

/* QToolBar */
QToolBar {
	border: 0px solid rgb(100, 100, 100);
	border-width: 0, 1px, 0, 0;
}

QToolBar::separator {
	width: 1px;
	background-color: rgb(100, 100, 100);
	margin: 5px, 5px, 20px, 20px;
}

QToolBar QLabel {
	font-size: 11pt;
}

/* Button  Component*/
QMainWindow QPushButton {
    background: rgb(60, 60, 60);
	border: 1px solid rgb(200, 200, 200);
	border-radius: 3px;
	color: rgb(200, 200, 200);
	padding: 0px, 0px, 20px, 20px;
	min-width: 50px;
}

QMainWindow QPushButton:hover {
	background: rgb(80, 80, 80);
}

QMainWindow QPushButton:pressed {
	background: rgb(200, 200, 200);
	color: rgb(20, 20, 20);
}

QMainWindow  QPushButton.component {
	font-size: 11pt;
	min-width: 100px;
	padding: 5px, 5px;
}

/* Scrollbar */
QScrollBar {
    background-color: rgb(50, 50, 50);
	border: 1px solid rgb(100, 100, 100);
	border-radius: 4px;
}

QScrollBar:horizontal {
    height: 15px;
}

QScrollBar:vertical {
    width: 15px;
}

QScrollBar::add-page, QScrollBar::sub-page {
    background: none;
}

QScrollBar::add-line {
	 border: none;
	 background: none;
	width: 0px;
	height: 0px;
}

QScrollBar::sub-line {
	 border: none;
	 background: none;
	width: 0px;
}

QScrollBar::handle {
    background: rgb(60, 60, 60);
    min-width: 10px;
	border: 1px solid rgb(200, 200, 200);
	border-radius: 3px;
}

QScrollBar::handle:hover {
    background: rgb(80, 80, 80);
}

QScrollBar::handle:pressed {
    background: rgb(200, 200, 200);
}

/* QSlider */
QSlider:horizontal {
	min-height: 20px;
}

QSlider::groove {
	background-color: rgb(50, 50, 50);
	border: 1px solid rgb(100, 100, 100);
	height: 10px;
	border-radius: 3px;
}

QSlider::sub-page {
	background-color: rgb(200, 200, 200);
	border-radius: 3px;
}

QSlider::handle {
	background-color: rgb(50, 50, 50);
	border: 1px solid rgb(200, 200, 200);
	border-radius: 3px;
}

QSlider::handle:horizontal {
	width: 13px;
	margin-top: -5px;
	margin-bottom: -5px;
}

QSlider::handle:hover {
	background-color: rgb(80, 80, 80);
}

QSlider::handle:pressed {
	background-color: rgb(200, 200, 200);
	border-color: rgb(35, 35, 35);
}

/* QRadioButton */
QRadioButton::indicator::checked {
	color: rgb(200, 200, 200);
}</string>
